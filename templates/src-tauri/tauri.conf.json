{
	"$schema": "https://schema.tauri.app/config/2",
	"productName": "<%= name %>",
	"version": "<%= version %>",
	"identifier": "<%= identifier %>",
	"build": {
		"beforeDevCommand": "pnpm dev",
		"devUrl": "http://localhost:1420",
		"beforeBuildCommand": "pnpm build",
		"frontendDist": "../dist"
	},
	"app": {
		"windows": [
			{
				"title": "<%= name %>",
				"width": 600,
				"height": 400,
				"resizable": false,
				"fullscreen": false,
				"decorations": false
			}
		],
		"security": {
			"csp": null
		}
	},
	"bundle": {
		"active": true,
		"targets": "all",
		"icon": [
			"icons/32x32.png",
			"icons/128x128.png",
			"icons/128x128@2x.png",
			"icons/icon.ico"
		]
	}
}
